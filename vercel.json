{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" },
    {
      "src": "src/some-folder/**",
      "use": "@vercel/other",
      "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/index.js" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/generate-reflections.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}